  "businessServices": {
    "userManagement": {
      "name": "User Management Service",
      "description": "User CRUD operations, customer assignments, authentication",
      "basePath": "/admin/users",
      "endpoints": [
        {
          "id": "user-list",
          "method": "GET",
          "path": "/admin/users",
          "auth": true,
          "roles": ["admin", "superadmin"],
          "description": "Get all users in the system",
          "query": {
            "role": "optional - filter by role",
            "customerId": "optional - filter by customer"
          },
          "response": {
            "success": true,
            "data": {
              "users": [
                {
                  "id": "uuid",
                  "email": "user@example.com",
                  "name": "John Doe",
                  "role": "client_user",
                  "customers": ["customer-id"],
                  "created_at": "2025-01-01T00:00:00Z"
                }
              ]
            }
          }
        },
        {
          "id": "user-create",
          "method": "POST",
          "path": "/admin/users",
          "auth": true,
          "roles": ["admin", "superadmin"],
          "description": "Create new user",
          "body": {
            "email": "required - unique email",
            "name": "required - full name",
            "password": "required - min 8 characters",
            "role": "required - superadmin|admin|client_manager|client_user|tester|viewer",
            "customers": "optional - array of customer IDs"
          },
          "response": {
            "success": true,
            "data": {
              "user": {
                "id": "new-uuid",
                "email": "user@example.com"
              }
            },
            "message": "User created successfully"
          }
        },
        {
          "id": "user-update",
          "method": "PUT",
          "path": "/admin/users/:userId",
          "auth": true,
          "roles": ["admin", "superadmin"],
          "description": "Update user details",
          "params": {
            "userId": "User UUID"
          },
          "body": {
            "name": "optional",
            "role": "optional",
            "customers": "optional - array of customer IDs",
            "is_active": "optional - boolean"
          },
          "response": {
            "success": true,
            "message": "User updated successfully"
          }
        },
        {
          "id": "user-delete",
          "method": "DELETE",
          "path": "/admin/users/:userId",
          "auth": true,
          "roles": ["admin", "superadmin"],
          "description": "Delete user (soft delete)",
          "params": {
            "userId": "User UUID"
          },
          "response": {
            "success": true,
            "message": "User deleted successfully"
          }
        },
        {
          "id": "user-get-customers",
          "method": "GET",
          "path": "/admin/users/:userId/customers",
          "auth": true,
          "roles": ["admin", "superadmin"],
          "description": "Get user's assigned customers",
          "params": {
            "userId": "User UUID"
          },
          "response": {
            "success": true,
            "data": {
              "customers": [
                {
                  "id": "customer-uuid",
                  "name": "Acme Corp",
                  "assigned_at": "2025-01-01T00:00:00Z"
                }
              ]
            }
          }
        },
        {
          "id": "user-assign-customer",
          "method": "POST",
          "path": "/admin/users/:userId/customers/:customerId",
          "auth": true,
          "roles": ["admin", "superadmin"],
          "description": "Assign user to customer",
          "params": {
            "userId": "User UUID",
            "customerId": "Customer UUID"
          },
          "response": {
            "success": true,
            "message": "User assigned to customer successfully"
          }
        },
        {
          "id": "user-remove-customer",
          "method": "DELETE",
          "path": "/admin/users/:userId/customers/:customerId",
          "auth": true,
          "roles": ["admin", "superadmin"],
          "description": "Remove user from customer",
          "params": {
            "userId": "User UUID",
            "customerId": "Customer UUID"
          },
          "response": {
            "success": true,
            "message": "User removed from customer successfully"
          }
        }
      ]
    },
    
